{
  "expo": {
    "name": "Gogol Partner",
    "slug": "petcare-partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gogol-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": "petcare-partner",
    "splash": {
      "image": "./assets/images/gogol-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you select images for products and profile photos.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby pet services and clinics."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDmLE_PV6oEYOeN3u4PZmEFdsFAXRpxYVo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gogol-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.petcare.partner",
      "versionCode": 1,
      "minSdkVersion": 24,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDmLE_PV6oEYOeN3u4PZmEFdsFAXRpxYVo"
        }
      },
      "jsEngine": "hermes",
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "enableDangerousExperimentalLeanBuilds": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "ndkVersion": "25.1.8937393"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "build": {
      "production": {
        "android": {
          "buildType": "apk",
          "enableProguardInReleaseBuilds": true,
          "enableHermes": true,
          "enableShrinkResourcesInReleaseBuilds": true
        }
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4372cc5-9cba-4e88-b80e-45459889e16d"
      }
    },
    "owner": "mr_lone_nerd"
  }
}
